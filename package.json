    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.2",
    "ajv": "^6.10.1",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "empleo-nestjs-authentication": "0.0.0-rc.20190714-1721",
    "empleo-nestjs-common": "0.0.0-rc.20190714-1513",
    "empleo-nestjs-configuration": "0.0.0-rc.20190714-1512",
    "express-jwt": "^5.3.1",
    "helmet": "^3.18.0",
    "ioredis": "^4.11.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.5.1",
    "lodash": "^4.17.11",
    "p-event": "^4.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "read-pkg-up": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "^4.0.7",
    "tsconfig-paths": "3.8.0",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.4.1",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "^0.0.42",
    "@types/faker": "^4.1.5",
    "@types/helmet": "0.0.43",
    "@types/ioredis": "^4.0.12",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.135",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.12",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/read-pkg-up": "^3.0.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@types/yargs": "^13.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@empleo/nestjs-(.*)$": "<rootDir>/modules/empleo-$1"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  }
}
