  "scripts": {
    "clean": "rimraf coverage dist",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "start": "node -r tsconfig-paths/register dist/main.js",
    "start:dev": "nodemon --config nodemon.json",
    "start:debug": "nodemon --config nodemon-debug.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r test/entry.js 'src/**/*.spec.ts'",
    "test:all": "npm run test && npm run test:e2e",
    "test:coverage": "nyc npm test",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "mocha -r test/entry.js -t 10000 'test/**/*.e2e-spec.ts'"
  },
  "dependencies": {
    "@nestjs/common": "^6.5.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@types/node": "^10.14.12",
    "class-validator": "^0.9.1",
    "empleo-nestjs-authentication": "0.0.0-rc.20190714-1721",
    "empleo-nestjs-common": "0.0.0-rc.20190714-1513",
    "empleo-nestjs-configuration": "0.0.0-rc.20190714-1512",
    "empleo-nestjs-testing": "0.0.0-rc.20190716-1518",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "^4.0.7",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.2",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/faker": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
